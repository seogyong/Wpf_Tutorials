<Page x:Class="Lab1_Log.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lab1_Log"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page3">

    <Grid Background="#FFAC9A9A">
        <Button x:Name="Button_start" Content="Start" HorizontalAlignment="Left" Margin="48,75,0,0" VerticalAlignment="Top" Click="Button_start_Click" Height="20" Width="60"/>
        <Label x:Name="Label_from_variable" HorizontalAlignment="Left" Margin="26,117,0,0" VerticalAlignment="Top" Content="{Binding Name}" Height="40" Width="242" RenderTransformOrigin="-1.98,1.294"/>
        <Label Content="class.value --&gt; txtbox&#xA;debug console output : &quot;Debug.WriteLine&quot; for debug mode &quot;Trace.WriteLine&quot; for release mode" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="60" Width="712"/>

    </Grid>
</Page>
