<Window x:Class="Lab2_example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab2_example"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn_timer_start" Content="Timer Start" HorizontalAlignment="Left" Margin="205,87,0,0" VerticalAlignment="Top" Height="39" Width="83" Click="btn_timer_start_Click"/>
        <Button x:Name="btn_timer_stop" Content="Timer Stop" HorizontalAlignment="Left" Margin="317,87,0,0" VerticalAlignment="Top" Height="39" Width="83" Click="btn_timer_stop_Click"/>
        <RichTextBox x:Name="RichTextBox_state" Margin="37,231,0,0" Background="Black" Foreground="#FF04D109" HorizontalAlignment="Left" Width="664" Height="161" VerticalAlignment="Top" Cursor="IBeam" IsReadOnly="True" ScrollViewer.CanContentScroll="True" VerticalScrollBarVisibility="Auto" TextChanged="RichTextBox_state_TextChanged">
        </RichTextBox>
        <Label x:Name="label_title" Content="Label" HorizontalAlignment="Left" Margin="330,23,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
